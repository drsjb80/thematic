{
    "applications": {
        "gecko": {
            "id": "drsjb80@gmail.com",
            "strict_min_version": "57.0"
        }
    },
    "background": {
        "scripts": [
            "src/thematic.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/Thematic16.png",
            "32": "icons/Thematic32.png",
            "64": "icons/Thematic64.png"
        },
        "default_popup": "src/popup.html",
        "default_title": "Thematic"
    },
    "commands": {
        "Switch to default theme" : {
           "suggested_key": {
                "default": "Alt+Shift+D"
            } 
        },
        "Rotate to next theme" : {
           "suggested_key": {
                "default": "Alt+Shift+R"
            } 
        },
        "Toggle autoswitching" : {
           "suggested_key": {
                "default": "Alt+Shift+A"
            } 
        }

    },
    "default_locale": "en_US",
    "description": "__MSG_extensionDescription__",
    "author": "Steve Beaty",
    "developer": {
        "name": "Steve Beaty",
        "url": "https://github.com/drsjb80/thematic"
    },
    "icons": {
        "48": "icons/Thematic48.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "options_ui": {
        "browser_style": true,
        "page": "src/options.html"
    },
    "permissions": [
        "storage",
        "alarms",
        "menus",
        "management"
    ],
    "short_name": "__MSG_extensionShortName__",
    "version": "7.0.0.1"
}
